<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hh.repository.PreventRepository">
	<!-- AMB_SECTIONテーブルとバインドする -->
	<select id="getBranch" resultType="PreventListForm">
		SELECT DISTINCT(AMB_SECT_NM) FROM AMB_SECTION
	</select>
	<select id="getDept" resultType="PreventListForm">
		SELECT DISTINCT AMB_ALLSECT_NM FROM AMB_SECTION
	</select>	
</mapper>